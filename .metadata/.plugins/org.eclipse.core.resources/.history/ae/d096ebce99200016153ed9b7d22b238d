package game;

public class Board {
	enum Peice {
		PAWN, ROOK, KNIGHT, BISHOP, QUEEN
	}
	
	public volatile Piece[][] pieces = new Piece[8][8];
	public int num;
	
	public Board(int num) {
		this.num = num;
		
		for(int i = 0; i < 8;i++) {
			for(int j = 0; j < 8;j++) {
				pieces[i][j] = null;
			}
		}
		
		
	}
	
}
